extends layout

block head
    link(rel='stylesheet',href='/css/contact.css')
block content
    include commons/nav
    div(class='main')
        div(class='content')
            div(class='contact-cont')
                h1(id='1')
                    em(class='icon-us')
                    ='联系我们'
                div(class='cont',style='height:250px')
                    div(class='er-pic')
                        img(src='/imgs/contact/er.png')
                    p
                        b 公司名称:
                        ='杭州久拓软件有限公司'
                    p
                        b 公司地址:
                        ='杭州市滨江区春波路1288号东冠高新科技园1号楼105室'
                    p
                        b 公司邮箱:
                        ='innosoft@9tuo.com'
                    p
                        b 公司电话:
                        ='0571-87677660'
                h1(id='2')
                    em(class='icon-map')
                    ='地图导航'
                div(class='cont')
                    div(class='map')
                        //img(width='860' height='375' src='http://api.map.baidu.com/staticimage?width=860&height=375&center=120.206934,30.195157&markers=120.206934,30.195157&zoom=15&scale=2&markerStyles=-1,http://www.9tuo.com:8888/data/attachment/location.png')
                        img(width='860' height='375' src='/imgs/location.png')
    include commons/footer