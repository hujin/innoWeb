@charset "utf-8";
/* CSS Document */
.contact-cont{padding:30px 60px}
.contact-cont h1{font-size:30px; font-weight:bold;padding-bottom:20px; position:relative}
.contact-cont h1 em{width:32px; height:32px; display:block; position:absolute; left:-40px; top:8px; background-image:url('../imgs/web-icons.png'); background-repeat:no-repeat;}
.icon-us{background-position:0 -60px}
.icon-map{background-position:0 -30px}
.contact-cont .cont p{font-size:14px; line-height:28px}
.contact-cont .cont .er-pic{float:right; margin-right:80px; margin-top:-50px}
.contact-cont .cont .map{width:820px; height:375px}