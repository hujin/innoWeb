div(style='width:980px;margin:0 auto;position:relative')
    div(class='sub-nav')
        span(class='sub-nav-icon')
        each Menu in menus
            -if(Menu.child.length)
                ul
                    each subMenu in Menu.child
                        li
                            a(href=subMenu.url,target='_self')
                                =subMenu.name


