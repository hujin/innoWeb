extends layout

block head
    link(rel='stylesheet',href='/css/base.css')
    link(rel='stylesheet',href='/css/main.css')
    link(rel='stylesheet',href='/css/team.css')
    script(type='text/javascript',src='/js/jquery-debug.js')
    script(type='text/javascript',src='/js/commons.js')
block content
    include commons/nav
    div(class='main')
        div(class='content')
            div(class='team-cont')
                div(class='team-img')
                    img(src='imgs/team.png')
                ul(class='namelist')
                    li(class='list')
                        a(href='#name1',class='hover') UEDEnglish
                    li(class='list')
                        a(href='#name2') 开发English
                    li(class='list')
                        a(href='#name3') DBEnglish
                    li(class='list')
                        a(href='#name4') 商务English
                include team/team-list.jade


    include commons/footer