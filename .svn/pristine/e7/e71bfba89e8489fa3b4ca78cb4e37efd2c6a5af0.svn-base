div(class='right')
    div(class='mould')
        h3 最新资讯
        ul(class='list')
            each val,index in newNewsList
                li(class='news-p') ·
                    a(href='newshtml?id='+val.newID)=val.title
    div(class='mould')
        h3 同类推荐
        ul(class='list')
            each val,index in sameNewsList
                li(class='news-p') ·
                     a(href='newshtml?id='+val.newID)=val.title
    div(class='mould')
        img(src='../imgs/news/news-jpg.jpg')