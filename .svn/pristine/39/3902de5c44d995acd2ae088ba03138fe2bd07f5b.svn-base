.contact-cont{padding:30px 60px}
.contact-cont h1{font-size:30px; font-weight:bold;padding-bottom:10px; margin-bottom:20px; position:relative; border-bottom:2px #e3e3e3 solid}
.contact-cont h1 em{width:32px; height:32px; display:block; position:absolute; left:-40px; top:8px; background-image:url('../imgs/contact/web-icons.png'); background-repeat:no-repeat;}
.icon-us{background-position:0 -60px}
.icon-map{background-position:0 -30px}
.contact-cont .cont{ margin-top:30px}
.contact-cont .cont p{font-size:14px; line-height:35px; border-bottom:1px #e3e3e3 dotted ; width:450px}
.contact-cont .cont p b{padding-left:20px; margin-right:10px; background:url('../imgs/contact/icon-f.png') no-repeat left center}
.contact-cont .cont .er-pic{float:right; margin-right:80px;  }
.contact-cont .cont .map{width:860px; height:375px}