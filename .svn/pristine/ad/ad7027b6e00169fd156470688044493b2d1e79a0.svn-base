body{background:url('../imgs/bg.png') repeat;}

.header{
    width:100%;
    height:70px;
    background-color:#ffffff;
    background: -webkit-gradient(linear, left top, left bottom, from(#ffffff), to(#fafafa));
    background: -moz-linear-gradient(top, #ffffff, #fafafa);
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#fafafa');
    background: -o-linear-gradient(top, #ffffff, #fafafa);
    background: linear-gradient(top, #ffffff, #fafafa);
    border-bottom:1px solid #ffffff;
}
.nav{width:980px; height:70px; margin:0 auto; }
.nav .logo{float:left; width:281px;height:46px;margin-top:12px}
.nav .main-nav{ float:right; width:630px; padding-top:25px}
.nav .main-nav li{float:left; font-size:14px;  line-height:22px; height:41px;}
.nav .main-nav li a{padding:0 25px;border-right:1px #eee solid}
.nav .main-nav li .hover{color:#ff8523}

.sub-nav{
    width:120px;
    border-top:2px solid #f78719;
    position:absolute;
    top:-15px;
    left:430px;
    background:#fff;
    border-radius:0 0 3px 3px;
    z-index:9999;
    display:none;
}

.sub-nav-icon{
    height: 0;
    width: 0;
    border-width: 6px;
    border-style: dashed dashed solid dashed;
    border-color: transparent transparent #f78719 transparent;
    font-size: 0;
    position:absolute;
    top:-14px;
    left:37px;
}
.sub-nav ul{margin-top:5px}
.sub-nav li{height:27px}
.sub-nav li a{width:102px;height:26px;border-bottom:1px solid #eee;display:block;margin:0 4px;padding-left:10px;line-height:26px}
.sub-nav li:hover{background:#ebeff3}

.main{width:100%;border-top:2px solid #dcdddf}
.content{margin:0 auto;width:980px}

.banner-nav{width:100%; background:url('../imgs/sed-nav-bj.png') repeat-x; height:60px;position:fixed;bottom:0px;}
.banner-nav .tn-cont{width:980px; margin:0 auto; height:60px;text-align:center}
.banner-nav .tn-cont .tn-list{ line-height:60px; font-size:16px; text-align:center; display:inline-block;*display:inline;*zoom:1; width:130px; }
.banner-nav .tn-cont .tn-list:hover{background:url('../imgs/tn-list.png') repeat-x; color:#FFF}
.banner-nav .tn-cont .current{background:url('../imgs/tn-list.png') repeat-x; color:#FFF}


.footer{ background:#525665; height:130px}
.footer-cont{margin:0 auto; width:470px;height:130px}
.footer-cont .footer-qr{float:left;height:130px}
.footer-cont .footer-qr img{width:88px;height:88px;margin-top:21px}
.footer-cont .footer-tools{float:left;height:130px;padding-left:20px;width:357px}
.footer-tools .footer-menu{margin-top:35px;}
.footer-tools li{float:left;}
.footer-tools li a{color:#fff;padding:0 12px;border-right:1px solid #fff}
.footer-cont p{ line-height:25px; color:#FFF;padding-left:10px}